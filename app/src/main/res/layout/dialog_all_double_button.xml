<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="20dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="300dp"
            android:layout_height="146dp"
            android:padding="16dp">

            <TextView
                android:id="@+id/tv_dialog_double_title"
                style="@style/TextViewDialogTitle"
                app:layout_constraintBottom_toTopOf="@id/btn_dialog_double_cancel"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.2"
                tools:text="@string/dialog_register_success_title" />

            <TextView
                android:id="@+id/btn_dialog_double_cancel"
                style="@style/TextViewPrimary"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:text="@string/do_cancel"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/btn_dialog_double_accept"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1" />

            <Button
                android:id="@+id/btn_dialog_double_accept"
                style="@style/ButtonPrimary"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:text="@string/mypage_logout"
                app:layout_constraintBottom_toBottomOf="@id/btn_dialog_double_cancel"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/btn_dialog_double_cancel"
                app:layout_constraintTop_toTopOf="@id/btn_dialog_double_cancel"
                app:layout_constraintVertical_bias="1" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</layout>